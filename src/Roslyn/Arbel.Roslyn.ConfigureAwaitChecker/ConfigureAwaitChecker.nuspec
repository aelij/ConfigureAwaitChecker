<?xml version="1.0"?>
<package>
  <metadata>
    <id>ConfigureAwaitChecker</id>
    <version>0.1.0.0</version>
    <title>ConfigureAwait Checker</title>
    <authors>Eli Arbel</authors>
    <owners>Eli Arbel</owners>
    <licenseUrl>https://github.com/aelij/ConfigureAwaitChecker/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/aelij/ConfigureAwaitChecker/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Library code should use ConfigureAwait(false) with every await. Always specifying ConfigureAwait makes it clearer how the continuation is invoked and avoids synchonization bugs.</description>
    <releaseNotes></releaseNotes>
    <copyright></copyright>
    <tags>await async configureawait</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="*.dll" target="tools\analyzers\" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>